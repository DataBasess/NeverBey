package com.epro.infrastructure.security.bean;

import java.io.Serializable;

public class MenuBean implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = -8437444917521852980L;
	private Integer menuId;
	private String menuName;
	private String menuLink;
	private String flagAuthority;
	
	
	public Integer getMenuId() {
		return menuId;
	}
	public void setMenuId(Integer menuId) {
		this.menuId = menuId;
	}
	public String getMenuName() {
		return menuName;
	}
	public void setMenuName(String menuName) {
		this.menuName = menuName;
	}
	public String getFlagAuthority() {
		return flagAuthority;
	}
	public void setFlagAuthority(String flagAuthority) {
		this.flagAuthority = flagAuthority;
	}
	public String getMenuLink() {
		return menuLink;
	}
	public void setMenuLink(String menuLink) {
		this.menuLink = menuLink;
	}
	
	
}
